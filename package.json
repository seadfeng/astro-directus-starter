{
  "name": "astro-directus-starter.com",
  "version": "4.4.0",
  "scripts": {
    "start:frontend": "astro dev",
    "build": "astro check && astro build",
    "preview:frontend": "astro preview",
    "sync": "astro sync",
    "astro": "astro",
    "format:check": "prettier --check . --plugin=prettier-plugin-astro",
    "format": "prettier --write . --plugin=prettier-plugin-astro",
    "lint": "eslint .",
    "deploy": "npm run build && wrangler pages deploy dist",
    "prepare": "husky install",
    "develop:backend": "cd backend && docker compose up",
    "develop:frontend": "astro dev",
    "develop": "cross-env FORCE_COLOR=1 npm-run-all -l -p develop:*",
    "start:backend": "cd backend && docker compose up",
    "start": "cross-env FORCE_COLOR=1 npm-run-all -l -p start:*",
    "preview:backend": "cd backend && docker compose up",
    "preview": "cross-env FORCE_COLOR=1 npm-run-all -l -p preview:*"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.3",
    "@astrojs/cloudflare": "^11.0.4",
    "@astrojs/rss": "^4.0.7",
    "@directus/sdk": "^17.0.1",
    "@resvg/resvg-js": "^2.6.2",
    "astro": "^4.14.2",
    "axios": "^1.7.5",
    "fuse.js": "^7.0.0",
    "he": "^1.2.0",
    "lodash.kebabcase": "^4.1.1",
    "lucide-react": "^0.436.0",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "satori": "^0.10.14",
    "tailwindcss": "^3.4.6",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.5.4",
    "zod": "^3.23.8"
